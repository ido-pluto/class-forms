{
  "name": "class-forms",
  "version": "1.0.0",
  "description": "Express & React base framework for simple aspx-like forms",
  "main": "index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate-docs": "typedoc --plugin typedoc-plugin-missing-exports",
    "build": "tsc --build tsconfig.json --force",
    "prepack": "npm run build"
  },
  "keywords": [
    "express",
    "react",
    "form",
    "forms",
    "formidable",
    "form-framework",
    "aspx",
    "form-validation"
  ],
  "author": "Ido S.",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "fs-extra": "^11.1.1",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.17",
    "@types/formidable": "^3.4.2",
    "@types/fs-extra": "^11.0.1",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.3",
    "semantic-release": "^21.1.1",
    "typedoc": "^0.25.0",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typescript": "^5.2.2"
  }
}
